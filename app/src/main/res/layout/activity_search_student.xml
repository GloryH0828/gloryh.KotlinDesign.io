<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Activity.Admin.SearchStudentActivity">

    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/linearLayout21" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="100dp">
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
        >
            <TextView
                    android:text="按"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/textView25" android:textSize="20sp"
                    android:textColor="#000000"
                    android:gravity="center"/>
            <TextView
                    android:text="TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/sea_stu_key" android:textSize="20sp"
                    android:textColor="#3F51B5"
                    android:gravity="center"/>
            <Spinner
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/sea_stu_spinner"
            />
        </LinearLayout>
        <EditText
                android:layout_width="150dp"
                android:layout_height="47dp"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/sea_stu_index"
                android:textSize="20sp" android:textColor="#000000"
                android:gravity="center"/>
        <Button
                android:text="搜索"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/bt_sea_stu" android:layout_weight="1"
                android:background="#7B84BD" android:textSize="20sp"/>
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintTop_toBottomOf="@+id/linearLayout21"
            app:layout_constraintEnd_toEndOf="@+id/linearLayout21"
            app:layout_constraintStart_toStartOf="@+id/linearLayout21" android:id="@+id/linearLayout23"
            android:layout_marginTop="30dp">
        <TextView
                android:text="姓名："
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView33" android:textSize="20sp"
                android:textColor="#000000" android:layout_marginRight="30dp"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/sea_stu_name"
                android:textSize="20sp" android:textColor="#000000"
        />
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintTop_toBottomOf="@+id/linearLayout23"
            app:layout_constraintEnd_toEndOf="@+id/linearLayout23"
            app:layout_constraintStart_toStartOf="@+id/linearLayout23" android:id="@+id/linearLayout24"
            android:layout_marginTop="30dp">
        <TextView
                android:text="学号："
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView35" android:textSize="20sp"
                android:textColor="#000000" android:layout_marginRight="30dp"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/sea_stu_username" android:textSize="20sp"
                android:textColor="#000000"
        />
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintTop_toBottomOf="@+id/linearLayout24"
            app:layout_constraintEnd_toEndOf="@+id/linearLayout24" android:id="@+id/linearLayout25"
            app:layout_constraintStart_toStartOf="@+id/linearLayout24" android:layout_marginTop="30dp">
        <TextView
                android:text="性别："
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView37" android:textSize="20sp"
                android:textColor="#000000" android:layout_marginRight="30dp"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/sea_stu_sex" android:textSize="20sp"
                android:textColor="#000000"
        />
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/linearLayout22"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout25"
            app:layout_constraintEnd_toEndOf="@+id/linearLayout25"
            app:layout_constraintStart_toStartOf="@+id/linearLayout25" android:layout_marginTop="30dp">
        <TextView
                android:text="班级："
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView38" android:textSize="20sp"
                android:textColor="#000000" android:layout_marginRight="30dp"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/sea_stu_class" android:textSize="20sp"
                android:textColor="#000000"
        />
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintTop_toBottomOf="@+id/linearLayout22"
            app:layout_constraintEnd_toEndOf="@+id/linearLayout22"
            app:layout_constraintStart_toStartOf="@+id/linearLayout22" android:layout_marginTop="50dp"
            android:id="@+id/linearLayout26">
        <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp" app:srcCompat="@drawable/bt_previous"
                android:clickable="true"
                android:id="@+id/bt_sea_stu_previous"
                android:contentDescription="TODO" android:focusable="true"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:id="@+id/sea_stu_count" android:textSize="20sp"
                android:textColor="#000000" android:gravity="center" android:layout_marginRight="40dp"
                android:layout_marginLeft="40dp"/>
        <ImageView
                android:layout_width="40dp"
                android:layout_height="40dp" app:srcCompat="@drawable/bt_next"
                android:id="@+id/bt_sea_stu_next"
                android:clickable="true"
                android:contentDescription="TODO" android:focusable="true"/>
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/linearLayout27">
        <Button
                android:text="密码重置"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/sea_stu_reset" android:background="#9C27B0"
                android:textSize="20sp"
                android:layout_marginRight="50dp"/>
        <Button
                android:text="信息修改"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/sea_stu_edit" android:background="#9C27B0"
                android:textSize="20sp"
                android:layout_marginBottom="78dp" android:layout_marginRight="50dp"/>
        <Button
                android:text="信息删除"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/sea_stu_delete" android:layout_weight="1"
                android:background="#9C27B0" android:textSize="20sp"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>