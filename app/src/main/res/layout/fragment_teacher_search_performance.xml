<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".Fragment.Teacher.TeacherSearchPerformanceFragment">
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
                android:id="@+id/linearLayout42" android:layout_marginTop="50dp">
            <TextView
                    android:text="按"
                    android:layout_width="wrap_content"
                    android:layout_height="48dp"
                    android:id="@+id/textView41" android:textSize="20sp"
                    android:textColor="#000000"
                    android:gravity="center" android:layout_gravity="center"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="48dp"
                    android:id="@+id/tea_sea_per_key" android:textSize="20sp"
                    android:textColor="#2196F3" android:gravity="center" android:layout_gravity="center"/>
            <Spinner
                    android:layout_width="38dp"
                    android:layout_height="48dp"
                    android:id="@+id/spinner7"
                    android:gravity="center" android:layout_gravity="center"/>
            <EditText
                    android:layout_width="162dp"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:id="@+id/tea_sea_per_index"
                    android:textSize="20sp" android:hint="请输入关键字"
                    android:gravity="center"/>
            <Button
                    android:text="查询"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/bt_tae_sea_search" android:background="#3F51B5"
                    android:textSize="20sp"
            />
        </LinearLayout>
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout42"
                app:layout_constraintEnd_toEndOf="@+id/linearLayout42"
                app:layout_constraintStart_toStartOf="@+id/linearLayout42" android:layout_marginTop="40dp"
                android:id="@+id/tea_sea_layout">
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout43"
                    android:layout_gravity="center">
                <TextView
                        android:text="学号："
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView51" android:textSize="20sp"
                        android:textColor="#000000"
                        android:layout_marginRight="30dp"/>
                <TextView
                        android:text="TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/tea_sea_per_username" android:textSize="20sp"
                        android:textColor="#000000"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout44" android:layout_gravity="center" android:layout_marginTop="40dp">
                <TextView
                        android:text="姓名："
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView48" android:textSize="20sp"
                        android:textColor="#000000"
                        android:layout_marginRight="30dp"/>
                <TextView
                        android:text="TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/tea_sea_per_name" android:textSize="20sp"
                        android:textColor="#000000"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout45" android:layout_gravity="center" android:layout_marginTop="40dp">
                <TextView
                        android:text="班级："
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView54" android:textSize="20sp"
                        android:textColor="#000000"
                        android:layout_marginRight="30dp"/>
                <TextView
                        android:text="TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/tea_sea_per_class" android:textSize="20sp"
                        android:textColor="#000000"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout46" android:layout_gravity="center" android:layout_marginTop="40dp">
                <TextView
                        android:text="课程："
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView56" android:textSize="20sp"
                        android:textColor="#000000"
                        android:layout_marginRight="30dp"/>
                <TextView
                        android:text="TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/tea_sea_per_course" android:textSize="20sp"
                        android:textColor="#000000"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout47" android:layout_gravity="center" android:layout_marginTop="40dp">
                <TextView
                        android:text="成绩："
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView58" android:textSize="20sp"
                        android:textColor="#000000"
                        android:layout_marginRight="30dp"/>
                <TextView
                        android:text="TextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/tea_sea_per_number" android:textSize="20sp"
                        android:textColor="#000000"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center" android:layout_marginTop="40dp">
                <ImageView
                        android:src="@drawable/bt_previous"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:id="@+id/tea_sea_per_previous"
                        android:clickable="true" android:contentDescription="TODO" android:focusable="true"/>
                <TextView
                        android:text="30/30"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:id="@+id/tea_sea_per_count" android:textSize="20sp"
                        android:textColor="#000000" android:gravity="center" android:layout_marginRight="20dp"
                        android:layout_marginLeft="20dp"/>
                <ImageView
                        android:src="@drawable/bt_next"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:id="@+id/tea_sea_per_next"
                        android:clickable="true" android:contentDescription="TODO" android:focusable="true"/>
            </LinearLayout>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>